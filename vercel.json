{
  "buildCommand": "cd frontend && yarn install && ./node_modules/vite/bin/vite.js build",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "routes": [{ "src": "/api/[^.]+", "dest": "/api/", "status": 200 }]
}
